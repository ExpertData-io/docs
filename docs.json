{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "ExpertEngine",
  "appearance": {
    "default": "dark"
  },
  "colors": {
    "primary": "#50C9CE",
    "light": "#6DD5D9",
    "dark": "#0F2628",
    "background": {
      "dark": "#020617",
      "light": "#ffffff"
    }
  },
  "font": {
    "headings": {
      "family": "Sora",
      "weight": 600
    },
    "body": {
      "family": "Sora",
      "weight": 400
    }
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "quickstart",
              "architecture"
            ]
          },
          {
            "group": "Core Concepts",
            "pages": [
              "concepts/connectors",
              "concepts/tasks",
              "concepts/pipelines",
              "concepts/genie"
            ]
          },
          {
            "group": "Guides",
            "pages": [
              "guides/first-connector",
              "guides/first-pipeline",
              "guides/using-genie"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api/introduction",
              "api/authentication"
            ]
          },
          {
            "group": "Resources",
            "pages": [
              "api/connectors",
              "api/tasks",
              "api/pipelines",
              "api/executions"
            ]
          },
          {
            "group": "AI",
            "pages": [
              "api/genie"
            ]
          }
        ]
      },
      {
        "tab": "Deployment",
        "groups": [
          {
            "group": "Deployment",
            "pages": [
              "deployment/overview",
              "deployment/docker",
              "deployment/kubernetes",
              "deployment/aws"
            ]
          },
          {
            "group": "Configuration",
            "pages": [
              "deployment/environment",
              "deployment/security"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/ExpertData-io",
          "icon": "github"
        },
        {
          "anchor": "API Status",
          "href": "https://status.expertengine.io",
          "icon": "signal"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/expertengine-light.svg",
    "dark": "/logo/expertengine-dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@expertdata.io"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Launch App",
      "href": "https://app.expertengine.io"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "cursor"
    ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/ExpertData-io",
      "linkedin": "https://linkedin.com/company/expertdata"
    }
  },
  "rounded": "lg",
  "backgroundImage": "/images/nebula-bg.png"
}
